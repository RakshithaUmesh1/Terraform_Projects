pipeline {
 agent any 
     stages{
        stage("Checkout"){
              git branch : 'main' ,
                url: 'https://github.com/RakshithaUmesh1/Terraform_Projects.git'
  }
}
stage("Terraform init and plan"){
       steps{
         sh 'terraform init'
         sh 'terraform plan'
  }
}
stage("Terraform Syntax validate"){
    steps{
       sh 'terraform validate'
  }
}
stage("Terraform apply") {
     steps{
          sh 'terraform apply'
    }
  }

 }
}
